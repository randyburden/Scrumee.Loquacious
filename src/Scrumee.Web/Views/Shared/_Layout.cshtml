<!DOCTYPE html>
<html>
<head>
	<title>@ViewBag.Title</title>
    <link href="../../Content/Site.css" rel="stylesheet" type="text/css" />
	<script src="@Url.Content( "~/Scripts/jquery-1.5.1.min.js" )" type="text/javascript"></script>
	<script type="text/javascript">
	    $(document).ready(function () {

	        $("tr:nth-child(odd)").addClass("odd");

	    });
	</script>
</head>

<body>
	
	<div class="wrapper">

		<div class="content">
			<div class="header"><img src="@Url.Content( "~/Content/logo.png" )" alt="Scrumee" /></div>
		
			@RenderBody()
		</div>
	</div>
	<div class="footer">
		
        @Html.ActionLink("Add New User", "AddNewUser") <br /><br />

		Scrumee Project Management Solution &copy Randy Burden @DateTime.Now.Year.ToString(). All Rights Reserved.
	</div>
</body>
</html>
